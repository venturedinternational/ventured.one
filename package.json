{
  "name": "ventured-web",
  "version": "1.0.9",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "DEBUG=app* node tools/build/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "gh-deploy": "git subtree push --prefix dist origin gh-pages",
    "start": "DEBUG=app:* node_modules/.bin/webpack-dev-server --config tools/config/webpack.config --hot --inline",
    "test": "cross-env NODE_ENV=test  karma start tools/config/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "eslint client",
    "lint:watch": "nodemon -x 'npm run lint --fix'",
    "lint:fix": "npm run lint -- --fix; exit 0",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove"
  },
  "theme": "./theme.js",
  "prepush": {
    "tasks": [
      "npm run lint"
    ],
    "onDirtyState": "ask",
    "coloredOutput": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venturedinternational/ventured.one.git"
  },
  "author": "Ventured One <support@ventured.one>",
  "license": "MIT",
